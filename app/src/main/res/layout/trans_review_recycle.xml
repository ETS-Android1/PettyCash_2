            android:layout_height="wrap_content"
            android:weightSum="2"
            android:orientation="horizontal"
            android:paddingVertical="10dp"
            android:paddingHorizontal="5dp"
            >




            <TextView
                android:id="@+id/trans_review_line_billed"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                style="@style/normal_dark"
                android:text="@string/billed_to_customer"
                ></TextView>



            <TextView
                android:id="@+id/trans_review_line_billed_value"
                style="@style/normal_dark"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="test"
                android:textAlignment="textEnd"
                ></TextView>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2"
            android:orientation="horizontal"
            android:paddingVertical="10dp"
            android:paddingHorizontal="5dp"
            >




            <TextView
                android:id="@+id/trans_review_line_cbs_code"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                style="@style/normal_dark"
                android:text="@string/cbs_code"
                ></TextView>



            <TextView
                android:id="@+id/trans_review_line_cbs_code_value"
                style="@style/normal_dark"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="test"
